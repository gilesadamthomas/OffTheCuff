{% if paginate.pages > 1 %}
<div class="row">
  <div>
    <ul class="pagination pull-right delete-border-pagination">
    {% assign c = 0 %}   
          {% for part in paginate.parts %}
                <li><span>{{ part.title | link_to: part.url}}</span></li>  
                {% if collections.all.products.size/9 != c %}      
                <li><span>-</span></li>
                {% endif%}
          		{% assign c = c+1 %}
 			 {% endfor %}
    </ul>
  </div>
</div> 
{% endif %}


